
{
  "multipart": [
    { "apply": { "model": "crazyae2addons:block/research_cable_core" } },

    { "when": { "north": true },
      "apply": { "model": "crazyae2addons:block/research_cable_side" } },

    { "when": { "south": true },
      "apply": {
        "model": "crazyae2addons:block/research_cable_side",
        "y": 180
      }},

    { "when": { "west": true },
      "apply": {
        "model": "crazyae2addons:block/research_cable_side",
        "y": 270
      }},

    { "when": { "east": true },
      "apply": {
        "model": "crazyae2addons:block/research_cable_side",
        "y": 90
      }},

    { "when": { "up": true },
      "apply": {
        "model": "crazyae2addons:block/research_cable_side_vertical",
        "x": 0,
        "uvlock": true
      }},

    { "when": { "down": true },
      "apply": {
        "model": "crazyae2addons:block/research_cable_side_vertical",
        "x": 180,
        "uvlock": true
      }}
  ]
}
